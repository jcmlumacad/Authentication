{
    "name": "ucsdmath/authentication",
    "type": "library",
    "description": "A basic user authentication class for login.",
    "keywords": ["authentication", "ucsdmath", "component"],
    "homepage": "https://github.com/ucsdmath",
    "license": "MIT",
    "time": "2016-03-01",
    "authors": [
        {"name": "Daryl Eisner", "email": "deisner@ucsd.edu"}
    ],
    "support": {
        "source": "https://github.com/UCSDMath/Authentication",
        "email": "deisner@ucsd.edu"
    },
    "require": {
        "php": "^7.0",
        "ucsdmath/database": "1.7.x-dev",
        "ucsdmath/functions": "1.7.x-dev",
        "ucsdmath/encryption": "1.7.x-dev",
        "ucsdmath/persistence": "1.7.x-dev"
    },
    "require-dev": {
        "mockery/mockery": "0.9.4",
        "phpunit/phpunit": "5.2.10",
        "ucsdmath/testing": "1.7.x-dev",
        "squizlabs/php_codesniffer": "2.5.1"
    },
    "autoload": {
        "psr-4": {
            "UCSDMath\\Authentication\\": "src/Authentication/"
        }
    },
    "suggest": {
        "symfony/dependency-injection": "Symfony DependencyInjection Component for fast loading of classes"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.7.x-dev"
        }
